{
  "dependencies": [
    {
      "link": "https://github.com/matgla/EmbeddedUtilityLibraries.git",
      "type": "git",
      "version": "master",
      "target": "eul",
      "options": {
        "cmake_variables": {
          "DISABLE_TESTS": "ON"
        }
      }
    },
    {
      "link": "https://github.com/matgla/Yasld.git",
      "type": "git",
      "version": "main",
      "target": "yasld",
      "options": {
        "cmake_variables": {
          "YASLD_ARCH": "armv6-m",
          "YASLD_DISABLE_TESTS": "OFF"
        }
      }
    },
    {
      "link": "https://github.com/matgla/Renode_RP2040.git",
      "type": "git",
      "version": "main",
      "target": "renode_rp2040",
      "options": {
        "cmake_variables": {
        },
        "is_cmake_library": true
      }
    },
    {
      "link": "https://github.com/littlefs-project/littlefs.git",
      "type": "git",
      "version": "v2.9.1",
      "target": "littlefs",
      "options": {
        "create_library": {
          "type": "STATIC",
          "sources_filter": ["lfs.c", "lfs_util.c"],
          "sources_directory": "",
          "include_directories": ["."],
          "compile_definitions": ""
        }
      }
    }
  ]
}
